<plugin name='svn' version='1.0.2' grailsVersion='1.1 &gt; *'>
  <author>Peter Ledbrook</author>
  <authorEmail>pledbrook@vmware.com</authorEmail>
  <title>Subversion Plugin</title>
  <description>Provides SVNKit as a dependency; an SvnClient class that makes it easier to work with Subversion; and integration with the Release plugin.</description>
  <documentation>http://grails.org/plugin/svn</documentation>
  <type>SvnGrailsPlugin</type>
  <resources />
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='org.gmock' name='gmock' version='0.8.1' />
    </test>
    <build>
      <dependency group='org.tmatesoft.svnkit' name='svnkit' version='1.3.5' />
    </build>
  </dependencies>
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>